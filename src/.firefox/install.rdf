<?xml version="1.0" encoding="utf-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>youtubecenter@druggedhippo.id.au</em:id>
    <em:version>@version@</em:version>
    <em:type>2</em:type>
    <em:iconURL>resource://ytcenter/data/icon32.png</em:iconURL>
    <em:iconURL64>resource://ytcenter/data/icon64.png</em:iconURL64>
    <em:homepageURL>https://github.com/druggedhippo/YouTubeCenter/wiki</em:homepageURL>
    <em:optionsURL>http://www.youtube.com/#ytcenter.settings.open</em:optionsURL>
    <em:optionsType>3</em:optionsType>
    <em:bootstrap>true</em:bootstrap>

    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>@firefox-target-id@</em:id>
        <em:minVersion>@firefox-target-min-version@</em:minVersion>
        <em:maxVersion>@firefox-target-max-version@</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <!-- Seasmonkey -->
    <em:targetApplication>
      <Description>
        <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
        <em:minVersion>2.0</em:minVersion>
        <em:maxVersion>2.31</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <!-- Firefox Mobile -->
    <em:targetApplication>
      <Description>
        <em:id>@firefox-target-mobile-id@</em:id>
        <em:minVersion>@firefox-target-mobile-min-version@</em:minVersion>
        <em:maxVersion>@firefox-target-mobile-max-version@</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>@name@</em:name>
    <em:description>YouTube CenterDH contains all kind of different useful functions which makes your visit on YouTube much more entertaining.</em:description>
    <em:creator>Jeppe Rune Mortensen</em:creator>
    @firefox-update-install-part@
